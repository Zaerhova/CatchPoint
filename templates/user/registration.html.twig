{% extends 'base.html.twig' %}
{% block body %}
    <h1>Inscription</h1>
    {{ form_start(form) }}
    {{ form_row(form.username, {'label' : 'Nom d\'utilisateur : '}) }}
    {{ form_row(form.prenom , {'label' : 'Prenom : '}) }}
    {{ form_row(form.nom , {'label' : 'Nom : '}) }}
    {{ form_row(form.password , {'label' : 'Mot de passe : '}) }}
    {{ form_row(form.confirmPassword , {'label' : 'Confirmer votre mot de passe : '}) }}
    <button type="submit" class="btn btn-success">Inscription</button>
    {{ form_end(form) }}

{% endblock %}